@model Piranha.Areas.Manager.Models.SitemapModel
@{
    Layout = null;
    var tabindex = 3;
}

@foreach (var item in Model.Sitemap) {
    @Html.Partial("DisplayTemplates/SitemapItem", new Piranha.Areas.Manager.Models.SiteItemModel() {
        Item = item,
        SiteId = Model.SiteId,
        PageId = Model.PageId,
        ExpandedLevels = Model.ExpandedLevels,
        CurrentLevel = 0,
        ReadOnly = Model.ReadOnly,
        TabIndex = tabindex
    })
    tabindex += 4;
}
